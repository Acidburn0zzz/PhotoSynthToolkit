	ProjectType="Visual C++"
	Version="8.00"
	Name="PhotoSynthDownloadHelper"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(PhotoSynthDownloadHelper)\include"
		PreprocessorDefinitions="_WIN32_WINNT=0x0501;NOMINMAX"
	/>
